{
  "name": "backend",
  "version": "1.0.0",
  "description": "Coffee tracker backend API",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "prisma generate && prisma migrate deploy",
    "build:strict": "tsc",
    "start": "tsx src/index.ts",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate"
  },
  "keywords": [
    "api",
    "coffee",
    "tracker"
  ],
  "author": "Danny",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "@coffee/shared": "workspace:*",
    "@prisma/client": "5.22.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.4",
    "@types/passport-jwt": "^4.0.1",
    "nodemon": "^3.1.11",
    "prisma": "5.22.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
  }
}
